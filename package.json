{
  "name": "react-native-google-nav",
  "version": "0.1.0",
  "packageManager": "npm@10.9.0",
  "description": "React Native Google Navigation SDK wrapper for New Architecture (Turbo Modules + Fabric) with Expo support",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "import": {
        "types": "./lib/typescript/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "default": "./lib/module/index.js"
    },
    "./expo-plugin": {
      "types": "./plugin/build/index.d.ts",
      "default": "./plugin/build/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "plugin/build",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "bun run --filter react-native-google-nav-example",
    "clean": "del-cli lib plugin/build",
    "prepack": "bob build && tsc --project plugin/tsconfig.json",
    "build": "bob build",
    "build:plugin": "tsc --project plugin/tsconfig.json",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "echo \"No tests configured\"",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "google-maps",
    "navigation",
    "turn-by-turn",
    "google-navigation-sdk",
    "expo",
    "fabric",
    "turbo-module",
    "new-architecture"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JMilanya/react-native-google-nav.git"
  },
  "author": "JMilanya <codewithjosh777@gmail.com> (https://github.com/JMilanya)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JMilanya/react-native-google-nav/issues"
  },
  "homepage": "https://github.com/JMilanya/react-native-google-nav#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^8.57.1",
    "@expo/config-plugins": "^9.0.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@types/react": "^19.1.12",
    "del-cli": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-builder-bob": "^0.40.13",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.76.0",
    "expo": ">=54.0.0"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    }
  },
  "workspaces": [
    "example"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "GoogleNavViewSpec",
    "type": "all",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.googlenav"
    },
    "ios": {
      "components": {
        "GoogleNavView": {
          "className": "GoogleNavView"
        }
      }
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "create-react-native-library": {
    "type": "fabric-view",
    "languages": "kotlin-objc",
    "tools": [
      "eslint"
    ],
    "version": "0.57.0"
  },
  "dependencies": {}
}
